{
    "name": "silasyudi/inject-mocks",
    "description": "Automatic injection of mocks into test subjects via #InjectMocks and #Mock annotations, to speed up unit testing with PHPUnit.",
    "keywords": ["inject-mocks", "mocks", "tests", "testing-tools", "annotations", "phpunit"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Silas Yudi",
            "email": "syudik12@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "silasyudi/optional": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml.dist --testdox"
    },
    "autoload": {
        "psr-4": {
            "SilasYudi\\InjectMocks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilasYudi\\InjectMocks\\Tests\\": "tests/"
        }
    }
}
